{
    "name": "omaralalwi/php-builders",
    "description": "sample php traits to add ability to use builder design patterns with easy in PHP applications",
    "keywords": [
        "omaralalwi",
        "php-builders",
        "php",
        "php-patterns",
        "design-patterns",
        "builders",
        "design-patterns",
        "fluent-interface",
        "object-creation",
        "builder-pattern",
        "php-builders",
        "traits"
    ],
    "homepage": "https://github.com/omaralalwi/php-builders",
    "license": "MIT",
    "type": "library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "omar alalwi",
            "email": "omaralwi2010@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Omaralalwi\\PhpBuilders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omaralalwi\\PhpBuilders\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
